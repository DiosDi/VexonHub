-- 📂🟩Tatsumaki Pencere🟩📂
local TatsumakiCheats = PhantomForcesWindow:NewSection("Tatsumaki")

--🟩KillAll Tatsumaki🟩
KillingCheats:CreateTextbox("For KillAll Use Ult And Press", function(text)
end)
TatsumakiCheats:CreateButton("KillAll (ult)", function()

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local camera = game.Workspace.CurrentCamera

-- Function to teleport to each player
local function teleportToPlayers()
    -- Sabit kamera pozisyonunu ayarla
    local originalCameraType = camera.CameraType
    local originalCameraCFrame = camera.CFrame
    camera.CameraType = Enum.CameraType.Scriptable
    camera.CFrame = originalCameraCFrame

    -- Her oyuncuya teleport ol
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            wait(0.1)  -- 0.1 saniye bekleyin
        end
    end

    -- Belirli bir koordinata teleport ol
    local targetPosition = CFrame.new(0, 500000000, 0)  -- İlk hedef koordinatlar
    player.Character.HumanoidRootPart.CFrame = targetPosition

    wait(4)  -- 4 saniye bekleyin

    -- Farklı bir koordinata teleport ol
    local finalPosition = CFrame.new(100, 50, 100)  -- Son hedef koordinatlar
    player.Character.HumanoidRootPart.CFrame = finalPosition

    -- Kamera modunu eski haline getir
    wait(1)  -- Biraz bekleyin ki son teleport işlemi sonrası kamera değişikliği daha yumuşak olsun
    camera.CameraType = originalCameraType
    camera.CFrame = originalCameraCFrame
end

-- Call the function
teleportToPlayers()
end)


--🟩Bring Pixel🟩
TatsumakiCheats:CreateButton("Bring Pixel", function()
    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.One, false, game)
    game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.One, false, game)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    wait(0.3)
    local originalPosition = humanoidRootPart.Position
    humanoidRootPart.CFrame = CFrame.new(Vector3.new(999999999, 999999999, 999999999))
    wait(1)
    humanoidRootPart.CFrame = CFrame.new(originalPosition)
end)


