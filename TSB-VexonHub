local function teleportTo(location)
    -- Konum bilgilerini ayarla
    local locations = {
        Dummy = Vector3.new(150, 440, 30),
        Map = Vector3.new(150, 505, 30),
        SafeZone = Vector3.new(150, 705, 30),
        Pixel = Vector3.new(150, 999999999, 30),
        Void = Vector3.new(150, -495, -30),
        Montain = Vector3.new(155.577392578125,628.742431640625,-447.93841552734375),
        Counter = Vector3.new(-68, 29, 20346),
        ["Counter Up"] = Vector3.new(-78, 84, 20354),
        ["Atomic Base"] = Vector3.new(1063, 30, 23006),
        ["Atomic Base Up"] = Vector3.new(1063, 405, 23006),
        ["Atomic Slash"] = Vector3.new(1063, 131, 23006),
        Prison = Vector3.new(438, 439, -376)
    }
    
    -- Oyuncu karakterini al
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()

    -- Seçilen konuma teleport et
    if locations[location] then
        character:SetPrimaryPartCFrame(CFrame.new(locations[location]))
    else
        warn("Bilinmeyen konum: " .. location)
    end
end

