local TabMoveset = DrRayLibrary.newTab("MoveSets","http://www.roblox.com/asset/?id=9405930424");

TabMoveset.newButton("Copy Discord Link", "Join Dc For Free Bobuc :0", function()

local function copyTextToClipboard(text)
    local success, message = pcall(function()
        -- Windows kopyalama ilemi
        setclipboard(text)
    end)
    if not success then
        warn("" .. message)
    end
end

-- Kopyalanacak ï¿½rnek metin
copyTextToClipboard("https://discord.gg/Xm5sThnfsP")
end)

