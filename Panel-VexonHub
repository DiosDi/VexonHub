print("hello sir can you join to discord server")


--[[local CoreGui = game:GetService("StarterGui")
local setclipboard = setclipboard or toclipboard

CoreGui:SetCore("SendNotification", {
    Title = "VexonHub",
    Text = "There is a Queen Bee Giveaway in dc server rn. wanna join?",
    Duration = 10,
    Icon = "http://www.roblox.com/asset/?id=84519376661277",
    Button1 = "Ye Why Not"
})

local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        setclipboard("https://discord.gg/Xm5sThnfsP")
        CoreGui:SetCore("SendNotification", {
            Title = "VexonHub",
            Text = "Discord Server Link Copied To Clipboard <3",
            Icon = "http://www.roblox.com/asset/?id=84519376661277",
            Duration = 5
        })
    end
end)
]]
